{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"module": "commonjs",
		"target": "es2015",
		"declaration": true,
		"noImplicitAny": true,
		"preserveConstEnums": true,
		"removeComments": true,
		"sourceMap": true,
		"strictNullChecks": true,
		"moduleResolution": "node",
		"outDir": "dist"
	}
}
